{
  "name":"stefansl/contao-baguettebox",
  "description":"Baguettebox for Contao",
  "keywords":["contao","lightbox", "vanillajs"],
  "type": "contao-bundle",
  "homepage":"https://clickpress.de",
  "license":"MIT",
  "authors":[
    {
      "name":"CLICKPRESS",
      "homepage":"https://clickpress.de",
      "role":"Developer"
    }
  ],
  "require":{
    "php":">=7.1",
    "contao/core-bundle":"^4.4"
  },

  "autoload":{
    "psr-4": {
      "Clickpress\\BaguetteboxBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra":{
    "contao-manager-plugin": "Clickpress\\BaguetteboxBundle\\ContaoManager\\Plugin"
  }
}
